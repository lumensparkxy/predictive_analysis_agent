# Snowflake Analytics - Production Secrets
# SENSITIVE FILE - Store securely and never commit to version control
# Copy this file to production server as /opt/analytics/config/secrets.env
# Ensure proper file permissions: chmod 600 secrets.env

# Snowflake Authentication
SNOWFLAKE_PASSWORD=your_secure_snowflake_password
SNOWFLAKE_PRIVATE_KEY_PATH=/opt/analytics/config/snowflake_key.p8
SNOWFLAKE_PRIVATE_KEY_PASSPHRASE=your_private_key_passphrase

# Database Credentials
DATABASE_PASSWORD=your_secure_database_password
REDIS_PASSWORD=your_redis_password

# API Security
SECRET_KEY=your-256-bit-secret-key-for-production-use-random-generator
JWT_SECRET_KEY=your-jwt-signing-key-use-strong-random-string
ENCRYPTION_KEY=32-character-encryption-key-for-aes
API_MASTER_KEY=master-api-key-for-admin-access

# External Service Credentials
OPENAI_API_KEY=your_openai_api_key
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
PAGERDUTY_INTEGRATION_KEY=your_pagerduty_integration_key

# Email Configuration
SMTP_PASSWORD=your_smtp_password
SENDGRID_API_KEY=your_sendgrid_api_key

# Monitoring & Observability
DATADOG_API_KEY=your_datadog_api_key
NEW_RELIC_LICENSE_KEY=your_new_relic_license_key
GRAFANA_API_KEY=your_grafana_api_key

# Cloud Provider Credentials (if applicable)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
GCP_SERVICE_ACCOUNT_KEY_PATH=/opt/analytics/config/gcp-service-account.json
AZURE_CLIENT_SECRET=your_azure_client_secret

# SSL/TLS Certificates (if using private CA)
CA_CERT_PASSWORD=your_ca_cert_password

# Backup Encryption
BACKUP_ENCRYPTION_KEY=your-backup-encryption-key-32-chars

# OAuth/SSO Credentials (if applicable)
OAUTH_CLIENT_SECRET=your_oauth_client_secret
SAML_CERT_PASSWORD=your_saml_cert_password

# Database Encryption Keys
DB_ENCRYPTION_KEY=your-database-field-encryption-key
COLUMN_ENCRYPTION_KEY=your-column-level-encryption-key